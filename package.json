{
  "name": "delivery-web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8009",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate-schemas": "sta -p http://localhost:8080/api/v1/auth/openapi.json -o src/schemas/auth && sta -p http://localhost:8080/api/v1/todo/openapi.json -o src/schemas/todo && sta -p http://localhost:8080/api/v1/maps/openapi.json -o src/schemas/maps && sta -p http://localhost:8080/api/v1/packets/openapi.json -o src/schemas/packets"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/public-sans": "^4.5.11",
    "@fontsource/roboto": "^4.5.8",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.2",
    "@mui/x-data-grid": "^5.17.17",
    "axios": "^1.2.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.6.1",
    "sort-by": "^1.2.0",
    "swagger-typescript-api": "^12.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.3"
  }
}
